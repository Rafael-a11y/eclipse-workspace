package controle.ExerciciosEstruturaDeControle;

import java.util.Scanner;

public class NumeroPrimoSwitch {public static void main(String[] args) {
	Scanner entrada = new Scanner(System.in);
	System.out.print("Entre com um número inteiro: ");
	int numero = entrada.nextInt();
	boolean requisito = numero % 2 != 0 && numero % 3 != 0 && numero % 5 != 0 && numero % 7 != 0 numero == 2 || numero == 3 || numero == 5 || numero == 7;
	switch (requisito){
		case 1: System.out.println("Certo");
	}
	entrada.close();
}

}
