package classe;

public class DataTeste {
	
	public static void main(String[] args) {
		
		Data d1 = new Data();
		d1.ano = 2020;
		d1.mes = 06;
		d1.dia = 25;
		
		Data d2 = new Data();
		d2.ano = 2038;
		d2.mes = 03;
		d2.dia = 25;
		
		System.out.println(d1.dataFormatada());
		System.out.printf("%d/%d/%d",d2.dia,  d2.mes,  d2.ano);
		
	}

}
