package streams;

import java.util.function.Function;

public class Produto {
	String nome;
	double preco;
	double desconto;
	double frete;
	
	Produto(String nome, double preco, double desconto, double frete){
		this.nome = nome;
		this.preco = preco;
		this.desconto = desconto;
		this.frete = frete;
		if(preco <= 0) {
			this.preco = 0.0;
		}
		
	}
	
}
