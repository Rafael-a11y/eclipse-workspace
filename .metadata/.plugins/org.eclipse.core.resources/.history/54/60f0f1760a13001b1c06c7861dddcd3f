package streams;

import java.util.Arrays;
import java.util.List;
import java.util.function.BinaryOperator;

public class Reduce1 {
	public static void main(String[] args) {
		List<Integer> nums = Arrays.asList(1, 2, 3, 4, 5, 6, 7, 8, 9);
		BinaryOperator<Integer> totalidade = (acm, n) -> acm + n;
		/*Reduce é um método terminal, você pode usar BinaryOperator
		 * de parâmetro para esta função, geralmente usa-se uma 
		 * variável para se conseguir o valor de reduce*/
		int total = nums.stream().reduce(totalidade).get();
		System.out.println(total);
		Integer total2 = nums.parallelStream().reduce(100,totalidade);
		System.out.println(total2);
		//Resultado foi um Opcional<Integer>
		nums.stream().filter(n -> n > 5).reduce(totalidade)
		.ifPresent(System.out::println);
		
	}
}
